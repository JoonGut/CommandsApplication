package com.example.mobilcomandas;

import org.threeten.bp.LocalDateTime;

public class Articulos {
    private int id_articulo;
    private String nombre_articulo;
    private double precio_venta;
    private int cantidad_stock;
    private String ultima_mod_stock;
    private int IVA;
    private int id_familia;
    private String url;

    // Getters
    public int getId_articulo() { return id_articulo; }
    public String getNombre() { return nombre_articulo; }
    public double getPrecio() { return precio_venta; }
    public int getCantidad_stock() { return cantidad_stock; }
    public String getUltima_mod_stock() { return ultima_mod_stock; }
    public int getIVA() { return IVA; }
    public int getId_familia() { return id_familia; }
    public String getUrl() { return url; }

    // Setters
    public void setId_articulo(int id_articulo) { this.id_articulo = id_articulo; }
    public void setNombre(String nombre) { this.nombre_articulo = nombre; }
    public void setPrecio(double precio) { this.precio_venta = precio; }
    public void setCantidad_stock(int cantidad_stock) { this.cantidad_stock = cantidad_stock; }
    public void setUltima_mod_stock(String ultima_mod_stock) { this.ultima_mod_stock = ultima_mod_stock; }
    public void setIVA(int IVA) { this.IVA = IVA; }
    public void setId_familia(int id_familia) { this.id_familia = id_familia; }
    public void setUrl(String url) { this.url = url; }
}
